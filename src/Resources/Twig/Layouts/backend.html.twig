{% block body_params %} class="skin-blue-light sidebar-mini" {% endblock %}

{% block header %}
{% include 'Layouts/ag_header.html.twig' %}
{% endblock %}

{% block main_menu %}
     <aside class="main-sidebar">
         <section class="sidebar">

                <!-- sidebar menu: : style can be found in sidebar.less -->
                {% block menu %}

                {% endblock %}

             </section>
    </aside>
{% endblock %}

{% block container %}
{% endblock %}

{% block footer %}
{% include 'Layouts/ag_footer.html.twig' %}
{% endblock %}

